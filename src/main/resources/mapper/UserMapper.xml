<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.lorain.mybatis_demo.dao.mapper.UserMapper">
    <select id="selectUserByName" resultType="com.lorain.mybatis_demo.dao.model.User">
        select * from users where username = #{name}
    </select>
</mapper>
